        <nextcloud min-version="15" max-version="20"/>
    </dependencies>
    <settings>
        <admin>OCA\HancomOffice\Settings\Admin</admin>
        <admin-section>OCA\HancomOffice\Settings\Section</admin-section>
    </settings>
