<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>hancomoffice</id>
    <name lang="en">Hancom Office Connector</name>
    <summary lang="en">Enable seamless integration with Hancom Office Online and Hancom DocsConverter</summary>
    
    <description lang="en">The Hancom Office Online connector enables you to create, view, edit, and collaborate on MS Office compatible documents, spreadsheets and presentations within Nextcloud.  Multiple users can work easily together to create or co-edit documents in real-time.   </description>
    <licence>AGPL</licence>
    <author mail="global-support@hancom.com" homepage="https://office.hancom.com/">Hancom, Inc.</author>
    <version>0.0.9</version>
    <namespace>HancomOffice</namespace>
    <types>
        <filesystem/>
    </types>
    <documentation>
        <admin>https://github.com/hancom-git/hancomoffice#readme</admin>
        <developer>https://office.hancom.com</developer>
        <user>https://github.com/hancom-git/hancomoffice#readme</user>
    </documentation>
    <category>files</category>
    <category>integration</category>
    <category>office</category>
    <category>tools</category>
    <website>https://office.hancom.com</website>
    <discussion>https://office.hancom.com</discussion>
    <bugs>https://github.com/hancom-git/hancomoffice/issues</bugs>
    <repository type="git">https://github.com/hancom-git/hancomoffice.git</repository>
    <screenshot small-thumbnail="https://hancom-nextcloud-appstore.s3.ap-northeast-2.amazonaws.com/appstore-screenshots/main_small.png">
                                      https://hancom-nextcloud-appstore.s3.ap-northeast-2.amazonaws.com/appstore-screenshots/main.png</screenshot>
     <screenshot>https://hancom-nextcloud-appstore.s3.ap-northeast-2.amazonaws.com/appstore-screenshots/main_2.png</screenshot>
